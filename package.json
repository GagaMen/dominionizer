{
  "name": "dominionizer",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "22.21.1",
    "npm": "11.7.0"
  },
  "workspaces": [
    "tools/wiki-bot"
  ],
  "scripts": {
    "prepare": "husky",
    "start": "ng serve",
    "start:prod": "ng build && http-server -p 8080 -o -c-1 ./dist",
    "build": "ts-node ./scripts/build.ts",
    "build:changelog": "conventional-changelog -i CHANGELOG.md -s",
    "extract-i18n": "ng extract-i18n",
    "test": "ng test",
    "test:coverage": "npm run test -- --no-watch --code-coverage",
    "test:ci": "npm run test:coverage -- --no-progress --browsers=ChromeHeadlessCI",
    "eslint": "eslint . --ignore-path .gitignore --ext .ts,.js,.json,.component.html",
    "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --cache",
    "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@angular/animations": "21.0.6",
    "@angular/cdk": "21.0.5",
    "@angular/common": "21.0.6",
    "@angular/compiler": "21.0.6",
    "@angular/core": "21.0.6",
    "@angular/forms": "21.0.6",
    "@angular/material": "21.0.5",
    "@angular/platform-browser": "21.0.6",
    "@angular/platform-browser-dynamic": "21.0.6",
    "@angular/router": "21.0.6",
    "@angular/service-worker": "21.0.6",
    "chance": "1.1.12",
    "material-design-icons": "3.0.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2100.4",
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "^21.0.4",
    "@angular/cli": "21.0.4",
    "@angular/compiler-cli": "21.0.6",
    "@angular/language-service": "21.0.6",
    "@angular/localize": "21.0.6",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@types/chance": "1.1.6",
    "@types/jasmine": "5.1.7",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "commitizen": "4.3.1",
    "conventional-changelog-cli": "5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-unused-imports": "3.2.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "jasmine-core": "5.6.0",
    "jasmine-marbles": "0.9.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.4.3",
    "markdownlint-cli": "0.44.0",
    "prettier": "3.5.3",
    "stylelint": "16.15.0",
    "stylelint-config-sass-guidelines": "11.1.0",
    "stylelint-prettier": "5.0.3",
    "svgo": "3.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  }
}
