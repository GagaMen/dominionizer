{
    "name": "dominionizer",
    "version": "0.0.0",
    "engines": {
        "node": "^16",
        "npm": "^8"
    },
    "scripts": {
        "prepare": "husky install",
        "start": "ng serve",
        "start:prod": "npm run build && http-server -p 8080 -o -c-1 ./dist",
        "build": "ng build --localize",
        "build:changelog": "conventional-changelog -i CHANGELOG.md -s",
        "extract-i18n": "ng extract-i18n",
        "test": "ng test",
        "test:coverage": "npm run test -- --no-watch --code-coverage",
        "test:ci": "npm run test:coverage -- --no-progress --browsers=ChromeHeadlessCI",
        "eslint": "eslint . --ignore-path .gitignore --ext .ts,.js,.json,.component.html",
        "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --cache",
        "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore",
        "commitizen": "commitizen",
        "cm": "cz"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.1.0",
        "@angular/cdk": "^15.1.0",
        "@angular/common": "^15.1.0",
        "@angular/compiler": "^15.1.0",
        "@angular/core": "^15.1.0",
        "@angular/forms": "^15.1.0",
        "@angular/material": "^15.1.0",
        "@angular/platform-browser": "^15.1.0",
        "@angular/platform-browser-dynamic": "^15.1.0",
        "@angular/router": "^15.1.0",
        "@angular/service-worker": "^15.1.0",
        "@material/fab": "^14.0.0",
        "material-design-icons": "^3.0.1",
        "rxjs": "^7.5.6",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "^15.0.0-beta.0",
        "@angular-devkit/architect": "^0.1501.1",
        "@angular-devkit/build-angular": "^15.1.1",
        "@angular-eslint/builder": "15.2.0",
        "@angular-eslint/eslint-plugin": "15.2.0",
        "@angular-eslint/eslint-plugin-template": "15.2.0",
        "@angular-eslint/schematics": "15.2.0",
        "@angular-eslint/template-parser": "15.2.0",
        "@angular/cli": "^15.1.1",
        "@angular/compiler-cli": "^15.1.0",
        "@angular/language-service": "^15.1.0",
        "@angular/localize": "^15.1.0",
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@types/chance": "^1.1.1",
        "@types/jasmine": "^4.3.0",
        "@types/node": "^16.11.58",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "chance": "^1.1.7",
        "commitizen": "^4.2.5",
        "conventional-changelog-cli": "^2.2.2",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.3.6",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "http-server": "^14.1.1",
        "husky": "^8.0.1",
        "image-minimizer-webpack-plugin": "^3.4.0",
        "imagemin": "^8.0.1",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "jasmine-core": "^4.4.0",
        "jasmine-marbles": "^0.9.2",
        "jasmine-spec-reporter": "^7.0.0",
        "json-minimizer-webpack-plugin": "^4.0.0",
        "karma": "^6.4.0",
        "karma-chrome-launcher": "^3.1.1",
        "karma-coverage": "^2.2.0",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "lint-staged": "^13.0.3",
        "markdownlint-cli": "^0.33.0",
        "prettier": "^2.7.1",
        "stylelint": "^14.11.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "stylelint-prettier": "^2.0.0",
        "ts-node": "^10.9.1",
        "typescript": "4.8.4"
    }
}
