{
  "name": "dominionizer",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:prod": "npm run build && http-server -p 8080 -o ./dominionizer -c-1 ./dist",
    "build": "ng build --prod",
    "build:changelog": "conventional-changelog -i CHANGELOG.md -s",
    "test": "ng test",
    "test:coverage": "npm run test -- --no-watch --code-coverage",
    "eslint": "eslint . --ignore-path .gitignore --ext .ts,.js,.json,.component.html",
    "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --syntax scss --cache",
    "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore",
    "e2e": "ng e2e",
    "commitizen": "commitizen",
    "cm": "cz"
  },
  "lint-staged": {
    "*.{ts,js,json,component.html}": "eslint --ignore-path .gitignore",
    "*.scss": "stylelint --ignore-path .gitignore --syntax scss --cache",
    "*.md": "markdownlint --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@angular/service-worker": "~11.2.0",
    "@material/fab": "^9.0.0",
    "material-design-icons": "^3.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular-eslint/builder": "^0.8.0-beta.0",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.0",
    "@angular-eslint/schematics": "^0.8.0-beta.0",
    "@angular-eslint/template-parser": "^0.8.0-beta.0",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/language-service": "~11.2.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@types/chance": "^1.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/mathjs": "^6.0.7",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "chance": "^1.1.7",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-unused-imports": "^1.0.1",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.1",
    "markdownlint-cli": "0.24.0",
    "prettier": "2.1.2",
    "protractor": "~7.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-prettier": "1.1.2",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.5"
  }
}
