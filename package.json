{
    "name": "dominionizer",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "24.12.0",
        "npm": "11.6.2"
    },
    "workspaces": [
        "tools/wiki-bot"
    ],
    "scripts": {
        "prepare": "husky",
        "start": "ng serve",
        "start:prod": "ng build && http-server -p 8080 -o -c-1 ./dist",
        "build": "ts-node ./scripts/build.ts",
        "build:changelog": "conventional-changelog -i CHANGELOG.md -s",
        "extract-i18n": "ng extract-i18n",
        "test": "ng test",
        "test:coverage": "npm run test -- --no-watch --code-coverage",
        "test:ci": "npm run test:coverage -- --no-progress --browsers=ChromeHeadlessCI",
        "eslint": "eslint .",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check .",
        "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --cache",
        "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore"
    },
    "dependencies": {
        "@angular/animations": "21.0.6",
        "@angular/cdk": "21.0.5",
        "@angular/common": "21.0.6",
        "@angular/compiler": "21.0.6",
        "@angular/core": "21.0.6",
        "@angular/forms": "21.0.6",
        "@angular/material": "21.0.5",
        "@angular/platform-browser": "21.0.6",
        "@angular/platform-browser-dynamic": "21.0.6",
        "@angular/router": "21.0.6",
        "@angular/service-worker": "21.0.6",
        "chance": "1.1.13",
        "material-design-icons": "3.0.1",
        "rxjs": "7.8.2",
        "tslib": "2.8.1",
        "zone.js": "0.16.0"
    },
    "devDependencies": {
        "@angular-devkit/architect": "0.2100.4",
        "@angular-eslint/builder": "21.1.0",
        "@angular-eslint/eslint-plugin": "21.1.0",
        "@angular-eslint/eslint-plugin-template": "21.1.0",
        "@angular-eslint/schematics": "21.1.0",
        "@angular-eslint/template-parser": "21.1.0",
        "@angular/build": "21.0.4",
        "@angular/cli": "21.0.4",
        "@angular/compiler-cli": "21.0.6",
        "@angular/language-service": "21.0.6",
        "@angular/localize": "21.0.6",
        "@commitlint/cli": "20.3.0",
        "@commitlint/config-conventional": "20.3.0",
        "@eslint/compat": "2.0.0",
        "@eslint/eslintrc": "3.3.3",
        "@eslint/js": "9.39.2",
        "@eslint/json": "0.14.0",
        "@types/chance": "1.1.7",
        "@types/jasmine": "5.1.13",
        "@types/node": "24.1.0",
        "@typescript-eslint/eslint-plugin": "8.51.0",
        "@typescript-eslint/parser": "8.51.0",
        "angular-eslint": "21.1.0",
        "commitizen": "4.3.1",
        "conventional-changelog-cli": "5.0.0",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-unused-imports": "4.3.0",
        "globals": "17.0.0",
        "http-server": "14.1.1",
        "husky": "9.1.7",
        "jasmine-core": "5.13.0",
        "jasmine-marbles": "0.9.2",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.4",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "lint-staged": "16.2.7",
        "markdownlint-cli": "0.47.0",
        "prettier": "3.7.4",
        "stylelint": "16.26.1",
        "stylelint-config-standard-scss": "16.0.0",
        "stylelint-prettier": "5.0.3",
        "svgo": "4.0.0",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "typescript-eslint": "8.51.0"
    }
}
