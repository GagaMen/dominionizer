{
  "name": "dominionizer",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:prod": "npm run build && http-server -p 8080 -o ./dominionizer -c-1 ./dist",
    "build": "ng build",
    "build:changelog": "conventional-changelog -i CHANGELOG.md -s",
    "test": "ng test",
    "test:coverage": "npm run test -- --no-watch --code-coverage",
    "eslint": "eslint . --ignore-path .gitignore --ext .ts,.js,.json,.component.html",
    "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --syntax scss --cache",
    "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore",
    "e2e": "ng e2e",
    "commitizen": "commitizen",
    "cm": "cz"
  },
  "lint-staged": {
    "*.{ts,js,json,component.html}": "eslint --ignore-path .gitignore",
    "*.scss": "stylelint --ignore-path .gitignore --syntax scss --cache",
    "*.md": "markdownlint --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/material": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@angular/service-worker": "^14.2.1",
    "@material/fab": "^14.0.0",
    "material-design-icons": "^3.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.0.1",
    "@angular-devkit/architect": "^0.1402.2",
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/builder": "14.0.4",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/schematics": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@types/chance": "^1.1.1",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/mathjs": "^6.0.11",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "chance": "^1.1.7",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unused-imports": "^1.1.0",
    "http-server": "^0.12.3",
    "husky": "^5.0.9",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.26.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-prettier": "1.1.2",
    "ts-node": "^9.1.1",
    "typescript": "4.6.4"
  }
}