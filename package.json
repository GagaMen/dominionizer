{
  "name": "dominionizer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "eslint": "eslint . --ignore-path .gitignore --ext .ts,.js,.json,.component.html",
    "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --syntax scss --cache",
    "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore",
    "e2e": "ng e2e"
  },
  "lint-staged": {
    "*.{ts,js,json,component.html}": "eslint --ignore-path .gitignore",
    "*.scss": "stylelint --ignore-path .gitignore --syntax scss --cache",
    "*.md": "markdownlint --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.3",
