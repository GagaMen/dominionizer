{
    "name": "dominionizer",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "^22",
        "npm": "^11"
    },
    "workspaces": [
        "tools/wiki-bot"
    ],
    "scripts": {
        "prepare": "husky",
        "start": "ng serve",
        "start:prod": "ng build && http-server -p 8080 -o -c-1 ./dist",
        "build": "ts-node ./scripts/build.ts",
        "build:changelog": "conventional-changelog -i CHANGELOG.md -s",
        "extract-i18n": "ng extract-i18n",
        "test": "ng test",
        "test:coverage": "npm run test -- --no-watch --code-coverage",
        "test:ci": "npm run test:coverage -- --no-progress --browsers=ChromeHeadlessCI",
        "eslint": "eslint . --ignore-path .gitignore --ext .ts,.js,.json,.component.html",
        "stylelint": "stylelint \"**/*.scss\" --ignore-path .gitignore --cache",
        "markdownlint": "markdownlint \"**/*.md\" --ignore-path .gitignore"
    },
    "dependencies": {
        "@angular/animations": "^17.1.3",
        "@angular/cdk": "^17.1.2",
        "@angular/common": "^17.1.3",
        "@angular/compiler": "^17.1.3",
        "@angular/core": "^17.1.3",
        "@angular/forms": "^17.1.3",
        "@angular/material": "^17.1.2",
        "@angular/platform-browser": "^17.1.3",
        "@angular/platform-browser-dynamic": "^17.1.3",
        "@angular/router": "^17.1.3",
        "@angular/service-worker": "^17.1.3",
        "chance": "^1.1.11",
        "material-design-icons": "^3.0.1",
        "rxjs": "^7.5.6",
        "tslib": "^2.1.0",
        "zone.js": "^0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1701.3",
        "@angular-devkit/build-angular": "^17.1.3",
        "@angular-eslint/builder": "^17.2.1",
        "@angular-eslint/eslint-plugin": "^17.2.1",
        "@angular-eslint/eslint-plugin-template": "^17.2.1",
        "@angular-eslint/schematics": "^17.2.1",
        "@angular-eslint/template-parser": "^17.2.1",
        "@angular/cli": "^17.1.3",
        "@angular/compiler-cli": "^17.1.3",
        "@angular/language-service": "^17.1.3",
        "@angular/localize": "^17.1.3",
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@types/chance": "^1.1.1",
        "@types/jasmine": "^5.1.4",
        "@types/node": "^20.11.17",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "commitizen": "^4.2.5",
        "conventional-changelog-cli": "^4.1.0",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-unused-imports": "^3.0.0",
        "http-server": "^14.1.1",
        "husky": "^9.0.10",
        "jasmine-core": "^5.1.2",
        "jasmine-marbles": "^0.9.2",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.4.0",
        "karma-chrome-launcher": "^3.1.1",
        "karma-coverage": "^2.2.0",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "lint-staged": "^15.2.2",
        "markdownlint-cli": "^0.39.0",
        "prettier": "^3.2.5",
        "stylelint": "^16.2.1",
        "stylelint-config-sass-guidelines": "^11.0.0",
        "stylelint-prettier": "^5.0.0",
        "svgo": "^3.2.0",
        "ts-node": "^10.9.1",
        "typescript": "5.3.3"
    }
}
