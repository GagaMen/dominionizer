    "@angular/forms": "~10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "material-design-icons": "^3.0.1",
    "mathjs": "^6.6.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular-eslint/builder": "0.0.1-alpha.27",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.27",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.27",
    "@angular-eslint/template-parser": "0.0.1-alpha.27",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/mathjs": "^6.0.4",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
