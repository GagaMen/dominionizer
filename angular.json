{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "dominionizer": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "i18n": {
                "sourceLocale": "en",
                "locales": {
                    "zh": "src/locale/translations.zh.xlf",
                    "cs": "src/locale/translations.cs.xlf",
                    "nl": "src/locale/translations.nl.xlf",
                    "fi": "src/locale/translations.fi.xlf",
                    "fr": "src/locale/translations.fr.xlf",
                    "de": "src/locale/translations.de.xlf",
                    "el": "src/locale/translations.el.xlf",
                    "hu": "src/locale/translations.hu.xlf",
                    "it": "src/locale/translations.it.xlf",
                    "ja": "src/locale/translations.ja.xlf",
                    "ko": "src/locale/translations.ko.xlf",
                    "no": "src/locale/translations.no.xlf",
                    "pl": "src/locale/translations.pl.xlf",
                    "ro": "src/locale/translations.ro.xlf",
                    "ru": "src/locale/translations.ru.xlf",
                    "es": "src/locale/translations.es.xlf"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "assets": ["src/favicon.ico", "src/manifest.webmanifest", "src/CNAME"],
                        "styles": ["src/scss/styles.scss"],
                        "scripts": [],
                        "customWebpackConfig": {
                            "path": "extra-webpack.config.js"
                        }
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all",
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "serviceWorker": true,
                            "ngswConfigPath": "ngsw-config.json"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "localize": ["en"]
                        },
                        "zh": {
                            "localize": ["zh"]
                        },
                        "cs": {
                            "localize": ["cs"]
                        },
                        "nl": {
                            "localize": ["nl"]
                        },
                        "fi": {
                            "localize": ["fi"]
                        },
                        "fr": {
                            "localize": ["fr"]
                        },
                        "de": {
                            "localize": ["de"]
                        },
                        "el": {
                            "localize": ["el"]
                        },
                        "hu": {
                            "localize": ["hu"]
                        },
                        "it": {
                            "localize": ["it"]
                        },
                        "ja": {
                            "localize": ["ja"]
                        },
                        "ko": {
                            "localize": ["ko"]
                        },
                        "no": {
                            "localize": ["no"]
                        },
                        "pl": {
                            "localize": ["pl"]
                        },
                        "ro": {
                            "localize": ["ro"]
                        },
                        "ru": {
                            "localize": ["ru"]
                        },
                        "es": {
                            "localize": ["es"]
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "dominionizer:build:production"
                        },
                        "development": {
                            "buildTarget": "dominionizer:build:development"
                        },
                        "zh": {
                            "buildTarget": "dominionizer:build:development,zh"
                        },
                        "cs": {
                            "buildTarget": "dominionizer:build:development,cs"
                        },
                        "nl": {
                            "buildTarget": "dominionizer:build:development,nl"
                        },
                        "fi": {
                            "buildTarget": "dominionizer:build:development,fi"
                        },
                        "fr": {
                            "buildTarget": "dominionizer:build:development,fr"
                        },
                        "de": {
                            "buildTarget": "dominionizer:build:development,de"
                        },
                        "el": {
                            "buildTarget": "dominionizer:build:development,el"
                        },
                        "hu": {
                            "buildTarget": "dominionizer:build:development,hu"
                        },
                        "it": {
                            "buildTarget": "dominionizer:build:development,it"
                        },
                        "ja": {
                            "buildTarget": "dominionizer:build:development,ja"
                        },
                        "ko": {
                            "buildTarget": "dominionizer:build:development,ko"
                        },
                        "no": {
                            "buildTarget": "dominionizer:build:development,no"
                        },
                        "pl": {
                            "buildTarget": "dominionizer:build:development,pl"
                        },
                        "ro": {
                            "buildTarget": "dominionizer:build:development,ro"
                        },
                        "ru": {
                            "buildTarget": "dominionizer:build:development,ru"
                        },
                        "es": {
                            "buildTarget": "dominionizer:build:development,es"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "dominionizer:build",
                        "outputPath": "src/locale",
                        "outFile": "translations.xlf"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "styles": ["src/scss/styles.scss"],
                        "scripts": [],
                        "assets": ["src/assets", "src/favicon.ico", "src/manifest.webmanifest"],
                        "codeCoverageExclude": ["src/testing/**/*"]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "eslintConfig": ".eslintrc.js",
                        "tsConfig": ["tsconfig.app.json", "tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    },
    "schematics": {
        "@angular-eslint/schematics:application": {
            "setParserOptionsProject": true
        },
        "@angular-eslint/schematics:library": {
            "setParserOptionsProject": true
        }
    }
}
