        },
        {
            "source": "/:path((?!ko/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "KR"
                }
            ],
            "destination": "/ko/index.html"
        },
        {
            "source": "/:path((?!no/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "NO"
                }
