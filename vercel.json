            "destination": "/es/:path*",
            "permanent": false
        },
        {
            "source": "/:path",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "^(?!.*(CN|CZ|NL|FI|FR|DE|GR|HU|IT|JP|KR|NO|PL|RO|RU|ES)).*$"
                }
            ],
            "destination": "/en/:path*",
            "permanent": false
        }
    ],
    "trailingSlash": false
}
