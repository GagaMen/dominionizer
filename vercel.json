            "permanent": false
        },
        {
            "source": "/:path((?!fr/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "FR"
                }
            ],
            "destination": "/fr/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!de/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "DE"
                }
            ],
            "destination": "/de/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!el/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "GR"
                }
            ],
            "destination": "/el/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!hu/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "HU"
                }
            ],
            "destination": "/hu/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!it/).*)",
