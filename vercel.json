{
    "github": {
        "enable": false,
        "silent": true
    },
    "buildCommand": "npm run build",
    "devCommand": "npm start",
    "framework": "angular",
    "installCommand": "npm ci",
    "outputDirectory": "dist",
    "redirects": [
        {
            "source": "/:path((?!zh/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "CN"
                }
            ],
            "destination": "/zh/index.html"
        },
        {
            "source": "/:path((?!cs/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "CZ"
                }
            ],
            "destination": "/cs/index.html"
        },
        {
            "source": "/:path((?!nl/).*)",
