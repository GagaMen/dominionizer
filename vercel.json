                }
            ],
            "destination": "/pl/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!ro/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "RO"
                }
            ],
            "destination": "/ro/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!ru/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "RU"
                }
            ],
            "destination": "/ru/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!es/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "ES"
                }
