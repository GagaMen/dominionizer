                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "NL"
                }
            ],
            "destination": "/nl/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!fi/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "FI"
                }
            ],
