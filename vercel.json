{
    "github": {
        "enable": false,
        "silent": true
    },
    "buildCommand": "npm run build",
    "devCommand": "npm start",
    "framework": "angular",
    "installCommand": "npm ci",
    "outputDirectory": "dist",
    "redirects": [
        {
            "source": "/:path((?!zh/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "CN"
                }
            ],
            "destination": "/zh/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!cs/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "CZ"
                }
            ],
            "destination": "/cs/:path*",
            "permanent": false
        },
        {
            "source": "/:path((?!nl/).*)",
