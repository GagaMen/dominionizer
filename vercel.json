        {
            "source": "/:path((?!ru/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "RU"
                }
            ],
            "destination": "/ru/index.html"
        },
        {
            "source": "/:path((?!es/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "ES"
                }
            ],
            "destination": "/es/index.html"
        },
        {
            "source": "/:path(.*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "^(?!.*(CN|CZ|NL|FI|FR|DE|GR|HU|IT|JP|KR|NO|PL|RO|RU|ES)).*$"
                }
            ],
            "destination": "en/index.html"
        }
    ],
    "trailingSlash": false
