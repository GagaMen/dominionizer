        },
        {
            "source": "/:path((?!de/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "DE"
                }
            ],
            "destination": "/de/index.html"
        },
        {
            "source": "/:path((?!el/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "GR"
                }
            ],
            "destination": "/el/index.html"
        },
        {
            "source": "/:path((?!hu/).*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-ip-country",
                    "value": "HU"
                }
